{
    "version": 2,
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.3.1",
            "includeFiles": "api/*.php"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1.php"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/$1.php"
        },
        {
            "src": "/tutorial",
            "dest": "/api/tutorial.php"
        },
        {
            "src": "/public/(.*)",
            "dest": "/$1"
        }
    ]
}