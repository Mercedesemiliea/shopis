{
    "version": 2,
    "routes": [
        {
            "src": "/api/index.php",
            "dest": "/index.php",
            "headers": {
                "Location": "/index.php"
            },
            "status": 301
        },
        {
            "src": "/api/(.*)",
            "dest": "/$1"
        }
    ]
}