{
    "version": 2,
    "routes": [
        {
            "src": "/api/index.php",
            "headers": {
                "Location": "/index"
            },
            "status": 307
        },
        {
            "src": "/",
            "dest": "/api/products.php"
        },
        {
            "src": "/products",
            "dest": "/api/products.php"
        },
        {
            "src": "/public/(.*)",
            "dest": "/$1"
        }
    ]
}