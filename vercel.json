{
    "version": 2,
    "builds": [
        {
            "src": "api/*.php",
            "use": "@vercel/php"
        },
        {
            "src": "server/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/server/server.js"
        }
    ]
}