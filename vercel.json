{
    "version": 2,
    "builds": [
        {
            "src": "index.php",
            "use": "@vercel/php"
        },
        {
            "src": "products.php",
            "use": "@vercel/php"
        },
        {
            "src": "server/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/php/(.*)",
            "dest": "/php/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/server/server.js"
        }
    ]
}