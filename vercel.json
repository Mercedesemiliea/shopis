{
    "version": 2,
    "builds": [
        {
            "src": "server/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/index",
            "dest": "/api/index.php"
        },
        {
            "src": "/products",
            "dest": "/api/products.php"
        },
        {
            "src": "/public/(.*)",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "server/index.js"
        }
    ]
}